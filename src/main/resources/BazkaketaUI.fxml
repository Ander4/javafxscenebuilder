            <Font size="16.0" />
         </font></Label>
      <ImageView fx:id="imageBandera" fitHeight="49.0" fitWidth="70.0" layoutX="14.0" layoutY="27.0" pickOnBounds="true" preserveRatio="true" />
      <ImageView fx:id="imageLogo" fitHeight="89.0" fitWidth="156.0" layoutX="363.0" layoutY="7.0" pickOnBounds="true" preserveRatio="true" />
   </children>
