<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ehu.isad.controller.ui.BozkaketaKud">
   <children>
      <TableView fx:id="tblBoto" layoutX="10.0" layoutY="96.0" prefHeight="246.0" prefWidth="574.0">
        <columns>
          <TableColumn fx:id="banderaCol" prefWidth="75.0" />
          <TableColumn fx:id="herriCol" prefWidth="75.0" text="Herrialdea" />
            <TableColumn fx:id="artistaCol" prefWidth="75.0" text="Artista" />
            <TableColumn fx:id="abestiCol" prefWidth="75.0" text="Abestia" />
            <TableColumn fx:id="puntuCol" prefWidth="75.0" text="Puntuak" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <Button fx:id="btnBozkatu" layoutX="232.0" layoutY="360.0" mnemonicParsing="false" onAction="#onGorde" text="Bozkaketa Gorde" />
      <Label fx:id="lblTestua" layoutX="113.0" layoutY="19.0" prefHeight="58.0" prefWidth="210.0">
